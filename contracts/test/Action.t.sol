pragma solidity ^0.8.26;

import "forge-std/Test.sol";
import "./../src/KinoraOpenAction.sol";
import "./../src/KinoraMilestoneCheckLogic.sol";

contract ActionsTest is Test {
  KinoraOpenAction kinoraOpenAction;
  KinoraMilestoneCheckLogic kinoraMilestoneCheckLogic;

  address hub = address(1);
  address admin = address(3);
  address envoker = address(2);

  function setUp() public {
    vm.startPrank(admin);

    kinoraOpenAction = new KinoraOpenAction(
      hub,
      address(kinoraMilestoneCheckLogic)
    );
    kinoraMilestoneCheckLogic = new KinoraMilestoneCheckLogic();


    kinoraMilestoneCheckLogic.freezeSetKinoraOpenAction(
      address(kinoraOpenAction)
    );

    vm.stopPrank();
  }

  function testAction() public {
    KeyValue[] memory keys = new KeyValue[](1);
    keys[0] = KeyValue({
      key: keccak256("lens.param.configureKinora"),
      value: hex""
    });
    vm.startPrank(hub);
    kinoraOpenAction.configure(
      envoker,
      address(0),
      123,
      keys
    );
    vm.stopPrank();
  }
}
